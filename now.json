{
    "version": 2,
    "name": "HappyFenceProject-master",
    "routes": [
        {
            "src": "/api/hello",
            "dest": "./api/hello.js",
            "methods": ["POST"]
        }
    ],
    "env": {
        "MYSQL_HOST": "@mysql_host",
        "MYSQL_USER": "@mysql_user",
        "MYSQL_PASSWORD": "@mysql_password",
        "MYSQL_DATABASE": "@mysql_database",
        "MYSQL_PORT": "@mysql_port"
    },
    "regions": ["icn1"]
}